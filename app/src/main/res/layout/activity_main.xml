<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/btn_fast_click"
        android:layout_width="200dp"
        android:layout_height="100dp"
        android:text="fastClick"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btn_methodtime"
        android:layout_width="200dp"
        android:layout_height="100dp"
        android:text="methodeTime"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/btn_fast_click" />

    <Button
        android:id="@+id/btn_aop"
        android:layout_width="200dp"
        android:layout_height="100dp"
        android:text="Aop"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/btn_methodtime" />

    <Button
        android:id="@+id/btn_thread"
        android:layout_width="200dp"
        android:layout_height="100dp"
        android:text="thread"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/btn_aop" />

</androidx.constraintlayout.widget.ConstraintLayout>